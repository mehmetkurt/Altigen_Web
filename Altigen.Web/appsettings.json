{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "738e6622-dfaa-4a93-89c2-ed1b8362237a",
        "UseHttps": true,
        "SanitizeTinyMce": true,
        "HideTopLevelNodeFromPath": true,
        "TimeOut": "00:20:00",
        "DefaultUILanguage": "en-US",

        "UmbracoCssPath": "~/assets/styles",
        "UmbracoScriptsPath": "~/assets/scripts",
        "UmbracoMediaPath": "~/assets/media",
        "IconsPath": "~/umbraco/assets/icons",

        "ReservedUrls": "~/.well-known/",
        "ReservedPaths": "~/app_plugins/,~/install/,~/mini-profiler-resources/,~/umbraco/"
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "DeliveryApi": {
        "Enabled": true
      },
      "Runtime": {
        "Mode": "Production"
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  }
}
